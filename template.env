# Template environment variables for DAWN/AGNTCY implementation
# Copy this file to .env and replace with your actual API keys

# OpenAI API Configuration
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
OPENAI_ORG_ID=org-xxxxxxxxxxxxxxxx

# Anthropic API Configuration
ANTHROPIC_API_KEY=sk-ant-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Gemini API Configuration
GEMINI_API_KEY=AIza-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# GitHub API Configuration (optional but recommended - without it, rate limits are strict)
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Agent Configuration
DEFAULT_MODEL=gpt-4o
PRINCIPAL_AGENT_TYPE=openai  # options: openai, anthropic, gemini
TEMPERATURE=0.2
MAX_TOKENS=2048

# Service Configuration
ACP_SERVER_HOST=localhost
ACP_SERVER_PORT=8000
ENABLE_AUTH=false